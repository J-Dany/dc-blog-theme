{{#if this.excludeId }}
    {{#get "posts" filter="id:-{{this.excludeId}}" limit="5" order="published_at desc"}}
        {{#if posts}}
            {{> "partials/articles-section"}}
        {{/if}}
    {{/get}}
{{else}}
    {{#get "posts" limit="5" order="published_at desc"}}
        {{#if posts}}
            {{> "partials/articles-section"}}
        {{/if}}
    {{/get}}
{{/if}}

{{#if this.excludeId }}
    {{#get "posts" filter=(concat "id:-" this.excludeId "+tags:-['hash-not-visible', 'project']") include="tags" limit="10" order="published_at desc"}}
        {{#if posts}}
            {{> "partials/articles-section"}}
        {{/if}}
    {{/get}}
{{else}}
    {{#get "posts" filter="tags:-['hash-not-visible', 'project']" include="tags" limit="10" order="published_at desc"}}
        {{#if posts}}
            {{> "partials/articles-section"}}
        {{/if}}
    {{/get}}
{{/if}}

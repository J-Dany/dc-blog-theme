{{#if this.excludeId }}
    {{#get "posts" filter="id:-{{this.excludeId}}+tags:-['hash-not-visible']" include="tags" limit="5" order="published_at desc"}}
        {{#if posts}}
            {{> "partials/articles-section"}}
        {{/if}}
    {{/get}}
{{else}}
    {{#get "posts" filter="tags:-['hash-not-visible']" include="tags" limit="5" order="published_at desc"}}
        {{#if posts}}
            {{> "partials/articles-section"}}
        {{/if}}
    {{/get}}
{{/if}}
